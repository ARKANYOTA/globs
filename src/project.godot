[animation]

warnings/check_invalid_track_paths=false

[application]

config/name="Globs"
config/version="1.1"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0.14902, 0.168627, 0.266667, 1)
boot_splash/image="res://assets/images/logo_big.png"
boot_splash/fullsize=false
config/icon="res://assets/images/icon_big.png"

[autoload]

